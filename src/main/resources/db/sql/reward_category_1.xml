<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="addRewardCategory1">
        <![CDATA[
            INSERT INTO reward_category_1 (
                id,
                category,
                description
            )
            VALUES (
                :id,
                :category,
                :description
            )
        ]]>
    </entry>
    <entry key="getRewardCategory1ById">
        <![CDATA[
            SELECT
                id,
                category,
                description
            FROM
                reward_category_1
            WHERE
                id = :id
        ]]>
    </entry>

    <entry key="getAllRewardCategory1s">
        <![CDATA[
            SELECT
                id,
                category,
                description
            FROM
                reward_category_1
        ]]>
    </entry>
</properties>